    <script>
        let a = +prompt("nhập số :");
        let giaithua = 1;
        function tinhGiaiThua(a){
            for(i = 1 ; i<=a ; i++){
                giaithua = giaithua * i;
               
               
            } alert("giai thừa của " + a +" là " + giaithua)
        } tinhGiaiThua(a);
    </script>
